	keywords = {Humans, Health Services Accessibility, United States, Child, Preschool, Infant, Vaccination, Health Care Surveys, Income, Socioeconomic Factors, European Continental Ancestry Group, Social Class, African Americans, Censuses, Geography, Immunization Programs, New England},
	pages = {130--135}
