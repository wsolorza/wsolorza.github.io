	keywords = {Female, Humans, Male, Healthcare Disparities, United States, Adolescent, Adult, Middle Aged, Aged, Young Adult, Hispanic Americans, Cross-Sectional Studies, Vaccination, European Continental Ancestry Group, African Americans, Office Visits, Seasons, Influenza, Human, Influenza Vaccines},
