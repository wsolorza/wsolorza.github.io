	keywords = {Female, Humans, Male, United States, Adolescent, Adult, Middle Aged, Aged, Young Adult, Cross-Sectional Studies, Vaccination, Patient Acceptance of Health Care, Urban Population, Ethnic Groups, Influenza, Human, Influenza Vaccines},
